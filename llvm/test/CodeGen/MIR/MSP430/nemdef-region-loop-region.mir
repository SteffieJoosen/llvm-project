# RUN: llc -O3 -x=mir -start-before=msp430-nemesis-defender -msp430-nemesis-defender-enable=true -verify-machineinstrs -o - %s | FileCheck %s

--- |
  target triple = "msp430-unknown-unknown-elf"

  define i16 @foo(i16 "secret" %a, i16 %b) {
  entry:
    ret i16 0
  }

  declare i16 @next(...)

  declare void @bar(i16)
...
---
name: foo
body: |
  bb.0:
    successors: %bb.1, %bb.5
    PUSH16r killed $r10, implicit-def $sp, implicit $sp
    PUSH16r killed $r9, implicit-def $sp, implicit $sp
    PUSH16r killed $r8, implicit-def $sp, implicit $sp
    $r10 = MOV16rr $r13
    $r9 = MOV16rr $r12
    CMP16rr $r9, $r10, implicit-def $sr
    JCC %bb.5, 4, implicit $sr

  bb.1:
    $r8 = MOV16ri 1023

  bb.2:
    successors: %bb.3, %bb.4
    CALLi @next, implicit-def dead $r11, implicit-def $r12, implicit-def dead $r13, implicit-def dead $r14, implicit-def dead $r15, implicit-def dead $sr, implicit $sp
    $r12 = nsw ADD16rr killed $r12, $r10, implicit-def dead $sr
    CMP16rr killed $r12, $r9, implicit-def $sr
    JCC %bb.4, 4, implicit $sr

  bb.3:
    $r12 = MOV16rr $r10
    CALLi @bar, implicit-def dead $r11, implicit-def dead $r12, implicit-def dead $r13, implicit-def dead $r14, implicit-def dead $r15, implicit-def dead $sr, implicit $sp, implicit $r12

  bb.4:
    successors: %bb.5(0x04000000), %bb.2(0x7c000000)
    $r8 = nsw ADD16rc killed $r8, -1, implicit-def dead $sr
    CMP16rc $r8, 0, implicit-def $sr
    JCC %bb.2, 1, implicit $sr

  bb.5:
    $r12 = MOV16ri 3
    $r8 = POP16r implicit-def $sp, implicit $sp
    $r9 = POP16r implicit-def $sp, implicit $sp
    $r10 = POP16r implicit-def $sp, implicit $sp
    RET implicit $r12

